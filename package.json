    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "typescript": "~4.8.4",
    "n8n-workflow": "~1.5.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "n8n-workflow": "~1.5.0"
