{
  "name": "n8n-nodes-aitable",
  "version": "0.2.2",
  "description": "n8n node for Aitable API integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Hung Nguyen",
    "email": "hungtidus@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hungtidus/n8n-nodes-aitable.git"
  },
  "main": "index.js",
  "types": "src/type/custom.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && gulp build:icons",
    "dev": "tsc --watch",
